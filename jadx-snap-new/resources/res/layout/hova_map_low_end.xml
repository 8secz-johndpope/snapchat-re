<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:custom="http://schemas.android.com/custom" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/hova_page" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.snap.ui.view.TakeSnapButton android:id="@+id/camera_capture_button" android:background="@drawable/hova_nav_camera_dark_bg" app:camera_button_tint="@color/regular_orange" style="@style/hova_small_camera_button_low_end"/>
    <com.snap.ui.view.SnapHovaNavBarLayout android:layout_gravity="bottom" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="@dimen/hova_low_end_nav_bar_height" app:background="@drawable/hova_nav_background_low_end">
        <com.snap.hova.HovaNavIconContainerView android:id="@+id/memories_icon_container" android:layout_height="match_parent" android:layout_weight="1" app:force_show_label="true" app:icon_tint="@color/hova_light_mode_gray" app:page_type="2" style="@style/hova_icon_container_style_default_low_end"/>
        <com.snap.hova.HovaNavIconContainerView android:id="@+id/feed_icon_container" android:layout_height="match_parent" android:layout_weight="1" app:force_show_label="true" app:icon_tint="@color/hova_light_mode_gray" app:page_type="0" style="@style/hova_icon_container_style_default_low_end"/>
        <Space android:layout_weight="1" style="@style/hova_icon_container_style_default_low_end"/>
        <com.snap.hova.HovaNavIconContainerView android:id="@+id/discoverfeed_icon_container" android:layout_height="match_parent" android:layout_weight="1" app:force_show_label="true" app:icon_tint="@color/hova_light_mode_gray" app:page_type="1" style="@style/hova_icon_container_style_default_low_end"/>
        <com.snap.hova.HovaNavIconContainerView android:id="@+id/map_icon_container" android:layout_height="match_parent" android:layout_weight="1" app:force_show_label="true" app:icon_tint="@color/regular_orange" app:page_type="3" style="@style/hova_icon_container_style_default_low_end"/>
    </com.snap.ui.view.SnapHovaNavBarLayout>
</FrameLayout>
