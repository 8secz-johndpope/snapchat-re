<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/single_dp" android:color="@color/regular_blue"/>
            <solid android:color="@color/regular_blue"/>
            <corners android:radius="@dimen/legacy_scrollbar_radius"/>
        </shape>
    </item>
    <item android:left="@dimen/legacy_scrollbar_radius">
        <shape android:shape="rectangle">
            <stroke android:width="@dimen/single_dp" android:color="@color/regular_blue"/>
            <solid android:color="@color/regular_blue"/>
            <corners android:radius="@dimen/default_gap_quarter"/>
        </shape>
    </item>
</layer-list>
