<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:shape="rectangle">
    <item>
        <shape>
            <stroke android:width="@dimen/scan_content_card_border_width" android:color="@color/black_thirty_opacity"/>
            <padding android:left="@dimen/scan_content_card_border_width" android:top="@dimen/scan_content_card_border_width" android:right="@dimen/scan_content_card_border_width" android:bottom="@dimen/scan_content_card_border_width"/>
            <corners android:radius="@dimen/scan_content_card_radius"/>
        </shape>
    </item>
    <item>
        <shape android:layout_width="match_parent" android:layout_height="match_parent" android:shape="rectangle">
            <solid android:color="@color/white"/>
            <corners android:radius="@dimen/scan_content_card_radius"/>
        </shape>
    </item>
</layer-list>
